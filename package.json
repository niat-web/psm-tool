    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "zod": "^4.3.6"
  },
  "scripts": {
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "cross-env PORT=3000 react-scripts start",
    "start:server": "tsx backend/src/server.ts",
    "build:server": "tsc -p backend/tsconfig.json",
    "typecheck:client": "tsc -p tsconfig.json --noEmit",
    "typecheck:server": "tsc -p backend/tsconfig.json --noEmit",
    "typecheck": "npm run typecheck:client && npm run typecheck:server",
    "start": "npm run dev",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.2.3",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
